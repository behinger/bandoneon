{
  "env": {
    "amd": true
  },
  "rules": {
    "block-scoped-var": 2,
    "brace-style": 1,
    "consistent-this": [0, "self"],
    "curly": [2, "multi-line"],
    "complexity": [0, 11],
    "indent": [2, 4],
    "max-depth": [2, 6],
    "max-len": [1, 120, 4],
    "max-nested-callbacks": [2, 5],
    "no-console": 2,
    "no-empty": 0,
    "no-loop-func": 0,
    "no-undef": 2,
    "quotes": 0,
    "semi": [2, "always"],
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "strict": 0
  }
}
